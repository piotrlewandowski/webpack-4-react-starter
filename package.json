{
  "name": "webpack-4-react-starter",
  "version": "0.1.0",
  "description": "Simple Webpack 4 React Starter",
  "main": "webpack.config.base.babel.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --config webpack.config.dev.babel.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production --config webpack.config.prod.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piotrlewandowski/webpack-4-react-starter.git"
  },
  "keywords": [],
  "author": "Piotr Lewandowski <contact@piotrlewandowski.info> (piotrlewandowski.info)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/piotrlewandowski/webpack-4-react-starter/issues"
  },
  "homepage": "https://github.com/piotrlewandowski/webpack-4-react-starter#readme",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "react-hot-loader": "^4.12.19",
    "rimraf": "^3.0.0",
    "terser-webpack-plugin": "^2.3.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}

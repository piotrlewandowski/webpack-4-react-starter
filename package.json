{
  "name": "webpack-4-react-starter",
  "version": "1.3.1",
  "description": "Webpack 4 React Starter",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --config webpack.config.dev.babel.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production --config webpack.config.prod.babel.js",
    "eslint:check": "./node_modules/.bin/eslint --format friendly --ignore-path .gitignore --ext .js --ext .jsx src/",
    "eslint:fix": "./node_modules/.bin/eslint --fix --ext .js --ext .jsx --format friendly src/",
    "stylelint:check": "./node_modules/.bin/stylelint src/styles/**/*.scss",
    "stylelint:fix": "./node_modules/.bin/stylelint src/styles/**/*.scss --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piotrlewandowski/webpack-4-react-starter.git"
  },
  "keywords": [
    "webpack",
    "react",
    "react stater",
    "react boilerplate"
  ],
  "author": "Piotr Lewandowski <contact@piotrlewandowski.info> (piotrlewandowski.info)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/piotrlewandowski/webpack-4-react-starter/issues"
  },
  "homepage": "https://github.com/piotrlewandowski/webpack-4-react-starter#readme",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "terser-webpack-plugin": "^3.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.5.0",
    "webpack-notifier": "^1.12.0"
  }
}
